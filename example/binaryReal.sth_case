[global]
INPUT=  
EXITVAL=0

[default]
DRIVER=java_subroutine
OPTIONS="-L '*.j'" 
ENTRY=binaryReal

[case]
ARGS='10 \# "1234\0" . 4321'   
OUTPUT="2# 10011010010.01101110100111100001101"

[case]
ARGS='16 \# FACE . DEAF'
OUTPUT="2# 1111101011001110.11011110101010110011011"

[case]
ARGS='8 \# 1234 . 4300000'   
OUTPUT="2# 1010011100.100011"

[case]
ARGS='8 \# "1234\0" . 43'   
OUTPUT="2# 1010011100.10001010001111010111000"


